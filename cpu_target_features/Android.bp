package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

cc_binary {
    name: "cpu-target-features",
    srcs: [
        "main.cpp",
    ],
    generated_headers: ["print_target_features.in.cpp"],
}

genrule {
    name: "print_target_features.in.cpp",
    out: ["print_target_features.in.cpp"],
    tool_files: ["generate_printer.py"],
    cmd: "$(location generate_printer.py) $(out)",
}
