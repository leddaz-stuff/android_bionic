    visibility: [":__subpackages__"],
    license_kinds: ["SPDX-license-identifier-Apache-2.0","SPDX-license-identifier-BSD","SPDX-license-identifier-ISC","SPDX-license-identifier-MIT","legacy_unencumbered"],
